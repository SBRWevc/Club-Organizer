								<StackPanel Orientation="Horizontal"
												Margin="0 0 3 0">
									<materialDesign:PackIcon Kind="ContentSaveAddOutline"
																 Height="20"
																 Width="20"
																 Margin="0 0 5 0"/>
									<TextBlock Text="Сохранить"
												   Padding="0 2 0 0"/>
